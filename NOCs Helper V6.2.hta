'********************************************************************
'* Sleep - This subroutine open waits.
'********************************************************************
Sub Sleep (Timesec)
  Set objwsh = CreateObject("WScript.Shell") 
  objwsh.Run "Timeout /T " & Timesec & " /nobreak" ,0 ,true
  Set objwsh = Nothing
End Sub

'********************************************************************
'* RadwinSub - This subroutine opens Radwin Manager.
'********************************************************************
Sub RadwinSub
  dim strEXE
  strEXE = "RADWINManager.exe"
  strCommand = strEXE & " " & strIP
  Set objShell = CreateObject("Wscript.Shell")
  objShell.Run strCommand
End Sub

'********************************************************************
'* KillRadwin - This subroutine will kill all open Radwin Processes.
'********************************************************************
Sub KillRadwin
  strCommand = "cmd /c taskkill /f /IM RADWINManager.exe"
  Set objShell = CreateObject("Wscript.Shell")
  objShell.run(strCommand),7,True
End Sub

