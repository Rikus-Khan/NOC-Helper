'********************************************************************
'* GoHome - This subroutine closes all the above and reboots.
'********************************************************************
Sub GoHome
  KillRadwin()
  KillEdge()
  KillChrome()
  KillJava()
  KillGEarth()
  KillExcel()
  KillSnip()
  KillOutlook()
  KillSlack()
  KillTeams()
  
  strCommand = "shutdown -r -t 00"
  Set objShell = CreateObject("Wscript.Shell")
  objShell.run(strCommand),7,True 
End Sub

