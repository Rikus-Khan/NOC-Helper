'********************************************************************
Sub CLI
  Set objShell = CreateObject("Wscript.Shell")
  If txtTheme = "Default" Then objShell.Run "cmd.exe /k cd\ & prompt $t $P$G & color 07"
  If txtTheme = "Red" Then objShell.Run "cmd.exe /k cd\ & prompt $t $P$G & color 04"
