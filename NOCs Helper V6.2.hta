<SCRIPT LANGUAGE="VBScript">
'********************************************************************
'* Global Variables
'********************************************************************   
  Dim LinesNeeded, strUseField, strIP, CurrentTab, strColor1, strColor2, strColor3, strColor4, strColor5, strColor6, CSVArray(1000,3), IPLines, txtTheme, xPing, xRadwin, txtColor
'********************************************************************

'********************************************************************
'* Window_Onload
'********************************************************************
Sub Window_Onload
    self.Focus()
    self.ResizeTo 500,450
    document.title = "NOC's Little Friend V6.2 - < Author: Rick Stewart >"
    document.body.bgcolor="Silver"
    txtTheme = "Default"
    CustReset()
    MPReset()
    OpenTab1()

  '***************
  'The following lines read the IP file into memory.
  '***************
    
  'Set FileName to read IP info from
  csvfile = "IPRange.txt"

  'Prepare file system
  Set objFSO = CreateObject("Scripting.FileSystemObject")

  'Count the lines in the csvfile
  Set theFile = objFSO.OpenTextFile(csvfile, 8, True) 
  IPLines = theFile.Line

  'txtIP.value = IPLines

  'Open CSV file
  Set objFile = objFSO.OpenTextFile(csvfile)

  i = 0

  'Loop through each line of the file until the end.
  Do While objFile.AtEndOfStream <> True
    'This "splits" the line into another array creating a multi-Dimensional array.
    'CSVArray(0,0) will be everything before the first comma on the first row = Monopole
    'CSVArray(1,1) will be everything between the first and second commas on the second row = Layer
    'CSVArray(0,2) will be everything between the second and third commas on the first row = Sector
    'CSVArray(1,3) will be everything after the third comma on the second row = IP Address

    'This line will read one line of the CSV file and store it in LineArray.
    ' However, the Split command separates the values so that LineArray(0) gets the values before the first "," and so on.
    LineArray = Split(objFile.ReadLine, ",")
    For x = 0 to 3
      CSVArray(i,x) = LineArray(x)
    Next
    i = i + 1
  Loop

  'Memory wipe to free space
  Set objFSO = Nothing

  '***************
  'The above lines read the IP file into memory.
  '***************
End Sub

'********************************************************************
'* CustReset - This clears the values of the Customer text boxes.
'********************************************************************
Sub CustReset
  txtCustLat.value = ""
  txtCustLon.value = ""
  txtCustHeight.value = "(in meters)"
  OptionChoosera.disabled = true
  OptionChooserb.disabled = true
  OptionChooserc.disabled = true
  OptionChooserd.disabled = true
  btnCoordsSave.disabled = true
End Sub

'********************************************************************
'* MPReset - This clears the values of the Customer text boxes.
'********************************************************************
Sub MPReset
  OptionChoosera.selectedIndex = "0"
  MPChangeda()
  OptionChooserb.selectedIndex = "0"
  MPChangedb()
  OptionChooserc.selectedIndex = "0"
  MPChangedc()
  OptionChooserd.selectedIndex = "0"
  MPChangedd()
  btnCoordsSave.disabled = true
End Sub

'********************************************************************
'* CustChange - Activates the first dropdown when all three customer fields have info.
'********************************************************************
Sub CustChange
  If Len(txtCustLat.value & vbNullString) = 0 OR Len(txtCustLon.value & vbNullString) = 0 OR Len(txtCustHeight.value & vbNullString) = 0 Then
    'All Customer fields are empty, disable the MonoPole Selections
    OptionChoosera.disabled = true
    OptionChooserb.disabled = true
    OptionChooserc.disabled = true
    OptionChooserd.disabled = true
    btnCoordsSave.disabled = true
  Else
    OptionChoosera.disabled = false
    btnCoordsSave.disabled = false
  End If
End Sub

'********************************************************************
'* NewCoordsSave() - This subroutine will output a KML file.
'*  The purpose is to draw line of sight from points to a point.
'********************************************************************
Sub NewCoordsSave()

  'The below is a loop which checks all conditions and calls the appropriate subroutine to write the file.
  ' ***There is no error checking other than blank values.***
  If Len(txtCustLat.value & vbNullString) = 0 OR Len(txtCustLon.value & vbNullString) = 0 OR Len(txtCustHeight.value & vbNullString) = 0 Then
    'Send error for Customers Info
    MsgBox("Check Customers Info.")

  ElseIf Len(txtaMPLat.value & vbNullString) = 0 OR Len(txtaMPLon.value & vbNullString) = 0 OR Len(txtaMPHeight.value & vbNullString) = 0 Then
    'Send error for First MonoPoles Info
    MsgBox("Check First MonoPole Info.")

  ElseIf Len(txtbMPLat.value & vbNullString) = 0 OR Len(txtbMPLon.value & vbNullString) = 0 OR Len(txtbMPHeight.value & vbNullString) = 0 Then
    'Goto Write One Lines
    LinesNeeded = 1
    NewWriteLines()

  ElseIf Len(txtcMPLat.value & vbNullString) = 0 OR Len(txtcMPLon.value & vbNullString) = 0 OR Len(txtcMPHeight.value & vbNullString) = 0 Then
    'Goto Write Two Lines
    LinesNeeded = 2
    NewWriteLines()

  ElseIf Len(txtdMPLat.value & vbNullString) = 0 OR Len(txtdMPLon.value & vbNullString) = 0 OR Len(txtdMPHeight.value & vbNullString) = 0 Then
    'Goto Write Three Lines
    LinesNeeded = 3
    NewWriteLines()

  Else 
    'Goto Write Four Lines
    LinesNeeded = 4
    NewWriteLines()

  End If
End Sub

'********************************************************************
'* NewWriteLines
'*  This subroutine will create a new KML file for Google Earth.
'********************************************************************
Sub NewWriteLines

  'Filename of the New KML File.
  strFile = "LoS.kml"

  'KML formating
  ' This file was broken down into sections as there appeared to be a limit
  ' to the number of characters which could be written per line.
  ' The last line of each section stores each line of the KML file.
  strLine1 = "<?xml version=""1.0"" encoding=""UTF-8""?>"
  strLine2 = vbCrLf & "<kml xmlns=""http://www.opengis.net/kml/2.2"" xmlns:gx=""http://www.google.com/kml/ext/2.2"" xmlns:kml=""http://www.opengis.net/kml/2.2"" xmlns:atom=""http://www.w3.org/2005/Atom"">"
  strLine3 = vbCrLf & "<Document>"
  strLine4 = vbCrLf & vbTab & "<name>LoS.kml</name>"
  strLine5 = vbCrLf & vbTab & "<Style id=""inline"">"
  strLine6 = vbCrLf & vbTab & vbTab & "<LineStyle>"
  strLine7 = vbCrLf & vbTab & vbTab & vbTab & "<color>ff0000ff</color>"
  strLine8 = vbCrLf & vbTab & vbTab & vbTab & "<width>2</width>"
  strLine9 = vbCrLf & vbTab & vbTab & "</LineStyle>"
  strLine10 = vbCrLf & vbTab & "</Style>"
  strHeader1 = strLine1 & strLine2 & strLine3 & strLine4 & strLine5 & strLine6 & strLine7 & strLine8 & strLine9 & strLine10
  
  strLine1 = vbTab & "<Style id=""inline0"">"
  strLine2 = vbCrLf & vbTab & vbTab & "<LineStyle>"
  strLine3 = vbCrLf & vbTab & vbTab & vbTab & "<color>ff0000ff</color>"
  strLine4 = vbCrLf & vbTab & vbTab & vbTab & "<width>2</width>"
  strLine5 = vbCrLf & vbTab & vbTab & "</LineStyle>"
  strLine6 = vbCrLf & vbTab & "</Style>"
  strLine7 = vbCrLf & vbTab & "<StyleMap id=""inline1"">"
  strLine8 = vbCrLf & vbTab & vbTab & "<Pair>"
  strLine9 = vbCrLf & vbTab & vbTab & vbTab & "<key>normal</key>"
  strLine10 = vbCrLf & vbTab & vbTab & vbTab & "<styleUrl>#inline0</styleUrl>"
  strHeader2 = strLine1 & strLine2 & strLine3 & strLine4 & strLine5 & strLine6 & strLine7 & strLine8 & strLine9 & strLine10

  strLine1 = vbTab & vbTab & "</Pair>"
  strLine2 = vbCrLf & vbTab & vbTab & "<Pair>"
  strLine3 = vbCrLf & vbTab & vbTab & vbTab & "<key>highlight</key>"
  strLine4 = vbCrLf & vbTab & vbTab & vbTab & "<styleUrl>#inline</styleUrl>"
  strLine5 = vbCrLf & vbTab & vbTab & "</Pair>"
  strLine6 = vbCrLf & vbTab & "</StyleMap>"
  strLine7 = vbCrLf & vbTab & "<Placemark>"
  strLine8 =  vbCrLf & vbTab & vbTab & "<name>Line Measure</name>"
  strLine9 = vbCrLf & vbTab & vbTab & "<styleUrl>#inline1</styleUrl>"
  strLine10 = vbCrLf & vbTab & vbTab & "<LineString>"
  strLine11 =  vbCrLf & vbTab & vbTab & vbTab & "<extrude>1</extrude>"
  strLine12 = vbCrLf & vbTab & vbTab & vbTab & "<tessellate>1</tessellate>"
  strLine13 = vbCrLf & vbTab & vbTab & vbTab & "<altitudeMode>relativeToGround</altitudeMode>"
  strLine14 = vbCrLf & vbTab & vbTab & vbTab & "<coordinates>"
  strHeader3 = strLine1 & strLine2 & strLine3 & strLine4 & strLine5 & strLine6 & strLine7 & strLine8 & strLine9 & strLine10 & strLine11 & strLine12 & strLine13 & strLine14

  strLine1 = vbTab & vbTab & vbTab & "</coordinates>"
  strLine2 = vbCrLf & vbTab & vbTab & "</LineString>"
  strLine3 = vbCrLf & vbTab & "</Placemark>"
  strLine4 = vbCrLf & "</Document>"
  strLine5 = vbCrLf & "</kml>"
  strFooter = strLine1 & strLine2 & strLine3 & strLine4 & strLine5

  strSCoords = vbCrLf & vbTab & vbTab & vbTab & "</coordinates>"
  strSLineString = vbCrLf & vbTab & vbTab & "</LineString>"
  strSPlacemark = vbCrLf & vbTab & "</Placemark>"
  strPlacemark = vbCrLf & vbTab & "<Placemark>"
  strName =  vbCrLf & vbTab & vbTab & "<name>Line Measure</name>"
  strStyle = vbCrLf & vbTab & vbTab & "<styleUrl>#inline1</styleUrl>"
  strLineString = vbCrLf & vbTab & vbTab & "<LineString>"
  strExtrude =  vbCrLf & vbTab & vbTab & vbTab & "<extrude>1</extrude>"
  strTessellate = vbCrLf & vbTab & vbTab & vbTab & "<tessellate>1</tessellate>"
  strAltitudeMode = vbCrLf & vbTab & vbTab & vbTab & "<altitudeMode>relativeToGround</altitudeMode>"
  strCoords = vbCrLf & vbTab & vbTab & vbTab & "<coordinates>"
  strFormat = strSCoords & strSLineString & strSPlacemark & strPlacemark & strName & strStyle & strLineString & strExtrude & strTessellate & strAltitudeMode & strCoords & vbCrLf

  'String to add to the KML. It requires the TAB spacing and gets data from the HTML textbox elements which start with "txt"
  strFirstLine = vbTab & vbTab & vbTab & vbTab & txtaMPLon.value & "," & txtaMPLat.value & "," & txtaMPHeight.value & " " & txtCustLon.value & "," & txtCustLat.value & "," & txtCustHeight.value
  strSecondLine = strFormat & vbTab & vbTab & vbTab & vbTab & txtbMPLon.value & "," & txtbMPLat.value & "," & txtbMPHeight.value & " " & txtCustLon.value & "," & txtCustLat.value & "," & txtCustHeight.value
  strThirdLine = strFormat & vbTab & vbTab & vbTab & vbTab & txtcMPLon.value & "," & txtcMPLat.value & "," & txtcMPHeight.value & " " & txtCustLon.value & "," & txtCustLat.value & "," & txtCustHeight.value
  strFourthLine =  strFormat & vbTab & vbTab & vbTab & vbTab & txtdMPLon.value & "," & txtdMPLat.value & "," & txtdMPHeight.value & " " & txtCustLon.value & "," & txtCustLat.value & "," & txtCustHeight.value
 
  'Set variable to long function name.
  Set objFSO = CreateObject("Scripting.FileSystemObject")

  'Open temporary file for writing - Options are: (FileName, Open For Writing, Create New If Not Found).
  Set objTemp = objFSO.OpenTextFile(strFile, 2, True)
  
  'Write the headers to start the file
  objTemp.WriteLine strHeader1
  objTemp.WriteLine strHeader2
  objTemp.WriteLine strHeader3

  If LinesNeeded = 1 Then

    objTemp.WriteLine strFirstLine
    objTemp.WriteLine strFooter

    'Close the file
    objTemp.Close

    MsgBox("1 Lines Written.")    
  ElseIf LinesNeeded = 2 Then
    objTemp.WriteLine strFirstLine & strSecondLine
    objTemp.WriteLine strFooter

    'Close the file
    objTemp.Close

    MsgBox("2 Lines Written.")    
  ElseIf LinesNeeded = 3 Then
    objTemp.WriteLine strFirstLine & strSecondLine & strThirdLine
    objTemp.WriteLine strFooter

    'Close the file
    objTemp.Close

    MsgBox("3 Lines Written.")    
  Else
    objTemp.WriteLine strFirstLine & strSecondLine & strThirdLine & strFourthLine
    objTemp.WriteLine strFooter
    
    'Close the file
    objTemp.Close

    MsgBox("4 Lines Written.")    
  End If  
End Sub

